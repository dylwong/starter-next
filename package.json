{
  "name": "@app/sandbox",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "del components/polymorph && del styles/polymorph.css && del public/polymorph && del helpers/polymorph",
    "dev": "yarn polymorph && next dev",
    "build": "yarn polymorph && next build",
    "helpers": "yarn helpers:ensure-header-footer && yarn helpers:sitemap",
    "helpers:sitemap": "node helpers/generate-sitemap.js",
    "helpers:ensure-header-footer": "node helpers/ensure-header-and-footer.js",
    "lint": "next lint",
    "polymorph": "yarn run clean && dotenv -e ./.env -- polymorph build && yarn helpers",
    "start": "next start"
  },
  "dependencies": {
    "@forevolve/bootstrap-dark": "^2.1.0",
    "@meronex/icons": "^4.0.0",
    "bootstrap": "^5.1.3",
    "firebase-admin": "^10.0.0",
    "formik": "^2.2.9",
    "html-react-parser": "^1.4.3",
    "lodash": "^4.17.21",
    "next": ">=11.1.3",
    "node-fetch": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-use-copy-to-clipboard": "^1.0.1",
    "reactstrap": "^9.0.1",
    "sitemap-links": "^1.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@smarterlabs/polymorph": "0.2.19",
    "del-cli": "^4.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.2",
    "serve": "^13.0.2"
  }
}
