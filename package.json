{
  "name": "@smarterlabs/design-sync",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "bootstrap": "dotenv -e .env -- node ./helpers/bootstrap.js",
    "build": "yarn run bootstrap && dotenv -e .env -- next build",
    "build:start": "dotenv -e .env -- node helpers/build-start.js",
    "build:finish": "node ./helpers/build-finish.js",
    "build:error": "dotenv -e .env -- node helpers/build-errored.js",
    "build:netlify": "dotenv -e .env -- netlify build --offline",
    "deploy:netlify": "dotenv -e .env -- netlify deploy --prod",
    "netlify": "yarn build:netlify && yarn deploy:netlify",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@forevolve/bootstrap-dark": "^2.1.0",
    "@meronex/icons": "^4.0.0",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "cheerio": "^1.0.0-rc.10",
    "firebase-admin": "^10.0.0",
    "formik": "^2.2.9",
    "html-react-parser": "^1.4.3",
    "lodash": "^4.17.21",
    "next": ">=11.1.3",
    "node-fetch": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-use-copy-to-clipboard": "^1.0.1",
    "reactstrap": "^9.0.1",
    "sharp": "^0.29.3",
    "sitemap-links": "^1.0.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "del-cli": "^4.0.1",
    "dotenv-cli": "^4.0.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "lint-staged": "^12.1.2",
    "netlify-cli": "^8.6.0",
    "serve": "^13.0.2",
    "uglify-js": "^3.14.5"
  }
}
